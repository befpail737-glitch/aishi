<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f0f0"/>
  <g transform="scale(2)">
    <!-- Person -->
    <circle cx="200" cy="150" r="60" fill="#e2e8f0" stroke="#64748b" stroke-width="4"/>
    <!-- Body -->
    <rect x="170" y="210" width="60" height="100" fill="#3b82f6" stroke="#1d4ed8" stroke-width="3"/>
    <!-- Arms -->
    <rect x="150" y="220" width="20" height="10" fill="#e2e8f0" stroke="#64748b" stroke-width="2"/>
    <rect x="230" y="220" width="20" height="10" fill="#e2e8f0" stroke="#64748b" stroke-width="2"/>
    <!-- Legs -->
    <rect x="175" y="310" width="15" height="50" fill="#1e293b" stroke="#0f172a" stroke-width="2"/>
    <rect x="210" y="310" width="15" height="50" fill="#1e293b" stroke="#0f172a" stroke-width="2"/>
    <!-- Face details -->
    <circle cx="185" cy="140" r="5" fill="#64748b"/> <!-- Left eye -->
    <circle cx="215" cy="140" r="5" fill="#64748b"/> <!-- Right eye -->
    <path d="M195 160 Q200 170 205 160" stroke="#64748b" stroke-width="2" fill="none"/> <!-- Smile -->
    <!-- Engineer badge -->
    <rect x="180" y="240" width="40" height="20" fill="#fbbf24" stroke="#f59e0b" stroke-width="2" rx="3"/>
    <text x="200" y="255" font-size="10" text-anchor="middle" fill="#1e293b" font-family="Arial">FAE</text>
    <!-- Technical tools -->
    <line x1="140" y1="300" x2="160" y2="320" stroke="#4b5563" stroke-width="3"/>
    <line x1="160" y1="300" x2="140" y2="320" stroke="#4b5563" stroke-width="3"/>
    <!-- Circuit board in background -->
    <rect x="120" y="100" width="160" height="100" fill="#1e293b" stroke="#0f172a" stroke-width="4" rx="5"/>
    <!-- Components on circuit board -->
    <circle cx="150" cy="130" r="8" fill="#f87171"/>
    <rect x="180" y="125" width="20" height="10" fill="#a78bfa"/>
    <rect x="220" y="125" width="15" height="15" fill="#93c5fd" stroke="#3b82f6" stroke-width="1"/>
    <circle cx="250" cy="140" r="7" fill="#34d399"/>
    <!-- Capacitor symbol -->
    <g transform="translate(160, 160)">
      <line x1="0" y1="0" x2="40" y2="0" stroke="#94a3b8" stroke-width="3"/>
      <line x1="0" y1="15" x2="40" y2="15" stroke="#94a3b8" stroke-width="3"/>
      <line x1="20" y1="0" x2="20" y2="15" stroke="#94a3b8" stroke-width="3" stroke-dasharray="3,3"/>
    </g>
    <!-- Name tag -->
    <rect x="160" y="360" width="80" height="20" fill="white" stroke="#64748b" stroke-width="2" rx="3"/>
    <text x="200" y="375" font-size="12" text-anchor="middle" fill="#1e293b" font-family="Arial">Engineer Zhang</text>
    <!-- Label -->
    <text x="200" y="420" font-size="20" text-anchor="middle" fill="#4b5563" font-family="Arial">Field Application Engineer</text>
    <text x="200" y="440" font-size="16" text-anchor="middle" fill="#6b7280" font-family="Arial">Power Electronics Specialist</text>
  </g>
</svg>