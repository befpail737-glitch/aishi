<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f0f0"/>
  <g transform="scale(2)">
    <!-- Building exterior -->
    <rect x="100" y="120" width="200" height="160" fill="#e2e8f0" stroke="#64748b" stroke-width="4"/>
    <!-- Building windows -->
    <g fill="#7dd3fc">
      <!-- Left column of windows -->
      <rect x="115" y="130" width="15" height="20"/>
      <rect x="115" y="160" width="15" height="20"/>
      <rect x="115" y="190" width="15" height="20"/>
      <rect x="115" y="220" width="15" height="20"/>
      <rect x="115" y="250" width="15" height="20"/>
      <!-- Middle column of windows -->
      <rect x="170" y="130" width="15" height="20"/>
      <rect x="170" y="160" width="15" height="20"/>
      <rect x="170" y="190" width="15" height="20"/>
      <rect x="170" y="220" width="15" height="20"/>
      <rect x="170" y="250" width="15" height="20"/>
      <!-- Right column of windows -->
      <rect x="225" y="130" width="15" height="20"/>
      <rect x="225" y="160" width="15" height="20"/>
      <rect x="225" y="190" width="15" height="20"/>
      <rect x="225" y="220" width="15" height="20"/>
      <rect x="225" y="250" width="15" height="20"/>
    </g>
    <!-- Door -->
    <rect x="185" y="260" width="30" height="40" fill="#94a3b8" stroke="#64748b" stroke-width="2"/>
    <circle cx="190" y="280" r="3" fill="#fbbf24"/>
    <!-- Company sign -->
    <rect x="130" y="100" width="140" height="20" fill="#1a4e8c" rx="5"/>
    <text x="200" y="115" font-size="12" text-anchor="middle" fill="white" font-family="Arial">LiTong Group</text>
    <!-- Aishi distributor sign -->
    <rect x="140" y="270" width="120" height="15" fill="#3b82f6" rx="3"/>
    <text x="200" y="281" font-size="10" text-anchor="middle" fill="white" font-family="Arial">Authorized Aishi Distributor</text>
    <!-- People icons representing employees -->
    <g fill="#f8fafc" stroke="#64748b" stroke-width="1">
      <!-- Person 1 -->
      <circle cx="130" cy="310" r="8"/>
      <rect x="125" y="318" width="10" height="15"/>
      <!-- Person 2 -->
      <circle cx="150" cy="310" r="8"/>
      <rect x="145" y="318" width="10" height="15"/>
      <!-- Person 3 -->
      <circle cx="170" cy="310" r="8"/>
      <rect x="165" y="318" width="10" height="15"/>
      <!-- Person 4 -->
      <circle cx="190" cy="310" r="8"/>
      <rect x="185" y="318" width="10" height="15"/>
      <!-- Person 5 -->
      <circle cx="210" cy="310" r="8"/>
      <rect x="205" y="318" width="10" height="15"/>
    </g>
    <!-- Capacitor symbols -->
    <g transform="translate(100, 330)">
      <rect x="0" y="0" width="20" height="10" fill="#93c5fd" stroke="#3b82f6" stroke-width="1"/>
      <line x1="5" y1="0" x2="5" y2="-10" stroke="#4b5563" stroke-width="1"/>
      <line x1="15" y1="0" x2="15" y2="-10" stroke="#4b5563" stroke-width="1"/>
    </g>
    <g transform="translate(140, 330)">
      <rect x="0" y="0" width="20" height="10" fill="#93c5fd" stroke="#3b82f6" stroke-width="1"/>
      <line x1="5" y1="0" x2="5" y2="-10" stroke="#4b5563" stroke-width="1"/>
      <line x1="15" y1="0" x2="15" y2="-10" stroke="#4b5563" stroke-width="1"/>
    </g>
    <g transform="translate(180, 330)">
      <rect x="0" y="0" width="20" height="10" fill="#93c5fd" stroke="#3b82f6" stroke-width="1"/>
      <line x1="5" y1="0" x2="5" y2="-10" stroke="#4b5563" stroke-width="1"/>
      <line x1="15" y1="0" x2="15" y2="-10" stroke="#4b5563" stroke-width="1"/>
    </g>
    <!-- Label -->
    <text x="200" y="380" font-size="20" text-anchor="middle" fill="#4b5563" font-family="Arial">LiTong Group Office</text>
    <text x="200" y="400" font-size="16" text-anchor="middle" fill="#6b7280" font-family="Arial">Global Headquarters for Aishi Distribution</text>
  </g>
</svg>