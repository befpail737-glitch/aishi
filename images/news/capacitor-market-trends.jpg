    <text x="200" y="120" font-size="14" text-anchor="middle" fill="#1e293b" font-family="Arial">Capacitor Market Growth</text>
    <!-- Axes -->
    <line x1="120" y1="230" x2="280" y2="230" stroke="#64748b" stroke-width="2"/>
    <line x1="120" y1="230" x2="120" y2="140" stroke="#64748b" stroke-width="2"/>
    <!-- Bars -->
    <rect x="130" y="200" width="20" height="30" fill="#93c5fd"/>
    <rect x="160" y="180" width="20" height="50" fill="#60a5fa"/>
    <rect x="190" y="160" width="20" height="70" fill="#3b82f6"/>
    <rect x="220" y="150" width="20" height="80" fill="#1d4ed8"/>
    <rect x="250" y="145" width="20" height="85" fill="#1e40af"/>
    <!-- Bar labels -->
    <text x="140" y="245" font-size="10" text-anchor="middle" fill="#475569" font-family="Arial">2020</text>
    <text x="170" y="245" font-size="10" text-anchor="middle" fill="#475569" font-family="Arial">2021</text>
    <text x="200" y="245" font-size="10" text-anchor="middle" fill="#475569" font-family="Arial">2022</text>
    <text x="230" y="245" font-size="10" text-anchor="middle" fill="#475569" font-family="Arial">2023</text>
    <text x="260" y="245" font-size="10" text-anchor="middle" fill="#475569" font-family="Arial">2024</text>
    <!-- Growth arrow -->
    <path d="M280 230 L280 140 L275 145 L280 140 L285 145" fill="none" stroke="#22c55e" stroke-width="3"/>
    <!-- Capacitor icons -->
    <g>
      <rect x="105" y="105" width="10" height="15" fill="#d1d5db" stroke="#9ca3af" stroke-width="1"/>
      <line x1="107" y1="105" x2="107" y2="100" stroke="#4b5563" stroke-width="1"/>
      <line x1="113" y1="105" x2="113" y2="100" stroke="#4b5563" stroke-width="1"/>
    </g>
    <g transform="translate(40, 0)">
      <rect x="105" y="105" width="10" height="15" fill="#d1d5db" stroke="#9ca3af" stroke-width="1"/>
      <line x1="107" y1="105" x2="107" y2="100" stroke="#4b5563" stroke-width="1"/>
      <line x1="113" y1="105" x2="113" y2="100" stroke="#4b5563" stroke-width="1"/>
    </g>
    <g transform="translate(80, 0)">
      <rect x="105" y="105" width="10" height="15" fill="#d1d5db" stroke="#9ca3af" stroke-width="1"/>
      <line x1="107" y1="105" x2="107" y2="100" stroke="#4b5563" stroke-width="1"/>
      <line x1="113" y1="105" x2="113" y2="100" stroke="#4b5563" stroke-width="1"/>
    </g>
    <!-- Aishi logo -->
    <rect x="180" y="80" width="40" height="15" fill="#1a4e8c" rx="3"/>
    <text x="200" y="92" font-size="10" text-anchor="middle" fill="white" font-family="Arial">Aishi</text>
    <!-- Label -->
    <text x="200" y="330" font-size="20" text-anchor="middle" fill="#4b5563" font-family="Arial">Capacitor Market Trends & Analysis</text>
    <text x="200" y="350" font-size="16" text-anchor="middle" fill="#6b7280" font-family="Arial">Industry Insights & Growth Projections</text>
  </g>
