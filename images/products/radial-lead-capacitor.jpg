<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f0f0"/>
  <g transform="scale(4)">
    <!-- Radial Lead Capacitor -->
    <rect x="70" y="50" width="100" height="160" rx="10" fill="#d1d5db" stroke="#9ca3af" stroke-width="4"/>
    <!-- Top insulation -->
    <rect x="70" y="40" width="100" height="10" fill="#e5e7eb"/>
    <!-- Leads -->
    <line x1="85" y1="40" x2="85" y2="20" stroke="#4b5563" stroke-width="6"/>
    <line x1="135" y1="40" x2="135" y2="20" stroke="#4b5563" stroke-width="6"/>
    <!-- Capacitor symbol inside -->
    <line x1="90" y1="100" x2="140" y2="100" stroke="#4b5563" stroke-width="4"/>
    <line x1="90" y1="140" x2="140" y2="140" stroke="#4b5563" stroke-width="4"/>
    <line x1="115" y1="100" x2="115" y2="140" stroke="#4b5563" stroke-width="4" stroke-dasharray="4,4"/>
    <!-- Label -->
    <text x="120" y="240" font-size="20" text-anchor="middle" fill="#4b5563" font-family="Arial">Radial Lead Electrolytic Capacitor</text>
  </g>
</svg>