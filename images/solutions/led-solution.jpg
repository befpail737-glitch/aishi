<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f0f0"/>
  <g transform="scale(2)">
    <!-- LED Bulb -->
    <ellipse cx="200" cy="100" rx="60" ry="80" fill="#f8fafc" stroke="#64748b" stroke-width="4"/>
    <!-- LED Chip -->
    <circle cx="200" cy="120" r="15" fill="#22c55e"/>
    <!-- LED Light rays -->
    <g stroke="#22c55e" stroke-width="2">
      <line x1="200" y1="120" x2="240" y2="80"/>
      <line x1="200" y1="120" x2="250" y2="120"/>
      <line x1="200" y1="120" x2="240" y2="160"/>
      <line x1="200" y1="120" x2="160" y2="80"/>
      <line x1="200" y1="120" x2="150" y2="120"/>
      <line x1="200" y1="120" x2="160" y2="160"/>
    </g>
    <!-- Power Supply Circuit Board -->
    <rect x="100" y="180" width="200" height="100" rx="10" fill="#1e293b" stroke="#0f172a" stroke-width="4"/>
    <!-- Aishi Capacitors on PCB -->
    <rect x="120" y="200" width="30" height="20" fill="#93c5fd" stroke="#3b82f6" stroke-width="2"/>
    <rect x="160" y="200" width="30" height="20" fill="#93c5fd" stroke="#3b82f6" stroke-width="2"/>
    <rect x="200" y="200" width="30" height="20" fill="#93c5fd" stroke="#3b82f6" stroke-width="2"/>
    <rect x="240" y="200" width="30" height="20" fill="#93c5fd" stroke="#3b82f6" stroke-width="2"/>
    <!-- Circuit components -->
    <circle cx="135" y="240" r="10" fill="#f87171"/>
    <rect x="170" y="235" width="20" height="10" fill="#a78bfa"/>
    <rect x="210" y="235" width="20" height="10" fill="#a78bfa"/>
    <rect x="250" y="235" width="20" height="10" fill="#f87171"/>
    <!-- Aishi logo on PCB -->
    <text x="200" y="270" font-size="12" text-anchor="middle" fill="white" font-family="Arial">Aishi Capacitors</text>
    <!-- Label -->
    <text x="200" y="320" font-size="24" text-anchor="middle" fill="#4b5563" font-family="Arial">LED Lighting Solution with Aishi Capacitors</text>
  </g>
</svg>